knitr::opts_chunk$set(echo = TRUE)
gene_interaction <- data.frame(Gene = paste0('g', c(1:300)), stringsAsFactors = F)
part_1 <- lapply(c(1:300), function(x) {x = x+1; sample(gene_interaction$Gene, sample(1:10,1))})
part_2 <- lapply(c(1:300), function(x) {x = x+1; sample(gene_interaction$Gene, sample(1:5,1))})
gene_interaction$Rule <- unlist(lapply(c(1:1000), function(x) {paste0('( ',toString(unlist(part_1[x])), ' ) and not ( ', toString(unlist(part_2[x])), ' )')}))
